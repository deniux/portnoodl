tive Media Queries'].push(qId)\n      } else {\n        Noodl.Variables['Active Media Queries'] = Noodl.Variables['Active Media Queries'].filter(q => q !== qId)\n      }\n    }\n\n    this.createdQueries.push(qId)\n    this.queries[qId] = window.matchMedia(queryObject.query)\n    this.listeners[qId](this.queries[qId])\n    this.queries[qId].addEventListener('change', this.listeners[qId])\n  }\n\n  queryObjects.forEach((queryObject) => {\n    this.createListener(queryObject)\n  })    \n}\n\nNode.OnDestroy = function() {\n    if(this.createdQueries.length) {\n      this.createdQueries.forEach(qId => {\n        this.queries[qId].removeEventListener('change', this.listeners[qId])\n        this.queries[qId] = undefined\n        this.listeners[qId] = undefined\n        this.createdQueries = this.createdQueries.filter(query => query !== qId)\n      })\n    }\n}","scriptInputs":[],"scriptOutputs":[],"outtype-match":"boolean","outtype-noMatch":"boolean","intype-mediaQuery":"object","intype-mediaQueries":"array"},"ports":[],"children":[]},{"id":"549b8d36-7a89-a516-254d-7de0ff736957","type":"Variable2","parameters":{"name":"Active Media Queries"},"ports":[],"children":[]}],"connections":[],"ports":[],"roots":[]},{"name":"/Media Queries/Match Media Query","nodes":[{"id":"0b644b38-89b6-7be8-73f6-82afd5738653","type":"Component Outputs","parameters":{},"ports":[],"children":[]},{"id":"4ab1e9b0-a252-ecfe-1676-4c415631968c","type":"JavaScriptFunction","parameters":{"scriptInputs":[],"intype-mobileS":"boolean","intype-mobile":"boolean","intype-tablet":"boolean","intype-desktop":"boolean","intype-desktopL":"boolean","functionScript":"if (!Inputs.activeQueryArray) return\n\nOutputs['Matches Small Mobile'] = Inputs.activeQueryArray.includes('Small Mobile')\nOutputs['Matches Regular Mobile'] = Inputs.activeQueryArray.includes('Regular Mobile')\nOutputs['Matches Tablet'] = Inputs.activeQueryArray.includes('Tablet')\nOutputs['Matches Regular Desktop'] = Inputs.activeQueryArray.includes('R