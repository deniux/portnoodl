mages/img02.png","group":"Image","plug":"input","index":1},{"name":"Video","type":"*","plug":"input","index":2},{"name":"Mount","type":"boolean","default":true,"group":"General","plug":"input","index":3},{"name":"Scale","type":{"name":"number"},"default":1,"group":"Placement","plug":"input","index":4},{"name":"Opacity","type":"number","default":1,"group":"Style","plug":"input","index":5}],"roots":["e29dca59-7ffb-a178-d551-a28240c0a7c4"]},{"name":"/Media Queries/Media Query Setup","nodes":[{"id":"e4f494d4-aa80-ed54-99d0-fece1294ccf6","type":"Javascript2","parameters":{"code":"const queryObjects = [\n    {\n        \"name\": \"Small Mobile\",\n        \"query\": \"(max-width: 400px)\"\n    },\n    {\n        \"name\": \"Regular Mobile\",\n        \"query\": \"(min-width: 401px) and (max-width: 700px)\"\n    },\n    {\n        \"name\": \"Tablet\",\n        \"query\": \"(min-width: 701px) and (max-width: 1200px)\"\n    },\n    {\n        \"name\": \"Regular Desktop\",\n        \"query\": \"(min-width: 1201px) and (max-width: 1800px)\"\n    },\n    {\n        \"name\": \"Large Desktop\",\n        \"query\": \"(min-width: 1801px)\"\n    }\n]\n\nNode.OnInit = function() {\n  // Create a variable to hold global queries\n  Noodl.Variables['Active Media Queries'] = []\n\n  this.createdQueries = []\n  this.queries = {}\n  this.listeners = {}\n\n  this.createListener = (queryObject) => {\n    const qId = queryObject.name\n\n    this.listeners[qId] = query => {\n      if (query.matches) {\n        console.log(qId)\n        Noodl.Variables['Active Media Queries'].push(qId)\n      } else {\n        Noodl.Variables['Active Media Queries'] = Noodl.Variables['Active Media Queries'].filter(q => q !== qId)\n      }\n    }\n\n    this.createdQueries.push(qId)\n    this.queries[qId] = window.matchMedia(queryObject.query)\n    this.listeners[qId](this.queries[qId])\n    this.queries[qId].addEventListener('change', this.listeners[qId])\n  }\n\n  queryObjects.forEach((queryObject) => {\n    this.createListener(queryObject)\n  })    \n}\n\nNode.OnDestroy = function() {\n    if(this.createdQueries.length) {\n      this.createdQueries.forEach(qId => {\n        this.queries[qId].removeEventListener('change', this.listeners[qId])\n        this.queries[qId] = undefined\n        this.listeners[qId] = undefined\n        this.createdQueries = this.createdQueries.filter(query => query !== qId)\n      })\n    }\n}","scriptInputs":[],"scriptOutputs":[],"outtype-match":"boolean","outtype-noMatch":"boolean","intype-mediaQuery":"object","intype-mediaQueries":"array"},"ports":[],"children":[]},{"id":"549b8d36-7a89-a516-254d-7de0ff736957","type":"Variable2","parameters":{"name":"Active Media Queries"},"ports":[],"children":[]}],"connections":[],"ports":[],"roots":[]}]